{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$/*": ["../../*"],
      "@/interfaces/*": ["src/interfaces/*"],
      "@/entities/*": ["src/entities/*"],
      "@/modules/*": ["./src/modules/*"],
      "@/utils/*": ["./src/utils/*"]
    },
    "types": ["vite/client"]
  },
  "include": [
    "src/**/*.ts",
    "../../entities/*.ts"
  ]
}
